{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/server",
            "args": [
                "--pkey",
                "own/key.pem",
                "--cert",
                "own/cert.pem",
                "--port-num",
                "3000",
                "--vocab-path",
                "own/vocab.gz",
                "--www-root",
                "src/www",
                "--pkpass",
                "asdf"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build test"
        }
    ]
}
